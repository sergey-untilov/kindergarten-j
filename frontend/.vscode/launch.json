{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "Remote debugging",
          "sourceMaps": true,
          "url": "http://localhost:8080",
          "webRoot": "${workspaceRoot}",
          "runtimeExecutable": "/usr/bin/google-chrome-stable"
      },
      {
          "type": "node",
          "request": "launch",
          "name": "Launch Webpack",
          "program": "${workspaceRoot}/build/dev-server.js",
          "cwd": "${workspaceRoot}",
          "sourceMaps": true
      },
  ]
}
